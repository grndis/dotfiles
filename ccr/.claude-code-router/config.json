  "PROXY_URL": "",
  "OPENAI_API_KEY": "$OPENAI_API_KEY",
  "GEMINI_API_KEY": "$GEMINI_API_KEY",
  "OPENROUTER_KEY": "$OPENROUTER_KEY",
  "COPILOT_TOKEN": "$COPILOT_TOKEN",
  "GCLOUD_GEMINI": "$GCLOUD_GEMINI",
  "HOME": "$HOME",
  "transformers": [
    {
      "path": "/Users/grandis/.claude-code-router/plugins/gemini-cli.js",
      "options": {
        "project": "$GCLOUD_GEMINI"
      }
    },
    {
      "name": "",
      "path": "/Users/grandis/.claude-code-router/plugins/qwen-cli.js",
      "options": {}
    },
    {
      "name": "qwen-code",
      "path": "/Users/grandis/.claude-code-router/plugins/qwen-code.js",
      "options": {}
    },
    {
      "path": "$HOME/.claude-code-router/plugins/rovo-cli.js",
      "options": {}
    }
  ],
  "Providers": [
    {
      "name": "rovo-cli",
      "api_base_url": "https://api.atlassian.com/rovodev/v2/proxy/ai/v1/openai/v1/chat/completions",
      "api_key": "sk-xxx",
      "models": [
        "gpt-5-2025-08-07",
        "claude-sonnet-4@20250514"
      ],
      "transformer": {
        "use": [
          "rovo-cli"
        ]
      }
    },
    {
      "name": "gemini",
      "api_base_url": "https://generativelanguage.googleapis.com/v1beta/models/",
      "api_key": "$GEMINI_API_KEY",
      "models": [
        "gemini-2.5-pro",
        "gemini-2.5-flash"
      ],
      "transformer": {
        "use": [
          "gemini"
        ]
      }
    },
    {
      "name": "qwen-code",
      "api_base_url": "https://portal.qwen.ai/v1/chat/completions",
      "api_key": "$GEMINI_API_KEY",
      "models": [
        "qwen3-coder-plus"
      ],
      "transformer": {
        "use": [
          "enhancetool",
          "qwen-code"
        ]
      }
    },
    {
      "name": "qwen-cli",
      "api_base_url": "http://localhost",
      "api_key": "$GEMINI_API_KEY",
      "models": [
        "qwen3-coder-plus"
      ],
      "transformer": {
        "use": [
          "enhancetool",
          [
            "maxtoken",
            {
              "max_tokens": 1000000
            }
          ],
          "qwen-cli"
        ]
      }
    },
    {
      "name": "gemini-cli",
      "api_base_url": "https://cloudcode-pa.googleapis.com/v1internal",
      "api_key": "$GEMINI_API_KEY",
      "models": [
        "gemini-2.5-flash",
        "gemini-2.5-pro"
      ],
      "transformer": {
        "use": [
          "gemini-cli"
        ]
      }
    },
    {
      "name": "openrouter",
      "api_base_url": "https://openrouter.ai/api/v1/chat/completions",
      "api_key": "$OPENROUTER_KEY",
      "models": [
        "qwen/qwen3-coder",
        "qwen/qwen3-coder:free",
        "anthropic/claude-sonnet-4",
        "anthropic/claude-opus-4",
        "google/gemini-2.5-flash",
        "google/gemini-2.5-pro",
        "moonshotai/kimi-k2:free"
      ],
      "transformer": {
        "use": [
          "openrouter"
        ]
      }
    },
    {
      "name": "copilot",
      "api_base_url": "https://api.githubcopilot.com/chat/completions",
      "api_key": "$COPILOT_TOKEN",
      "models": [
        "gemini-2.5-pro",
        "claude-sonnet-4",
        "gpt-4.1",
        "gpt-4o",
        "gpt-4o-mini"
      ],
      "transformer": {
        "use": [
          "copilot"
        ]
      }
    },
    {
      "name": "qwen",
      "api_base_url": "http://192.168.31.2:4444/v1/chat/completions",
      "api_key": "$COPILOT_TOKEN",
      "models": [
        "qwen3-coder-plus"
      ],
      "transformer": {
        "use": [
          "qwen",
          "enhancetool"
        ]
      }
    },
    {
      "name": "openai",
      "api_base_url": "https://api.openai.com/v1/chat/completions",
      "api_key": "$OPENAI_API_KEY",
      "models": [
        "gpt-5",
        "gpt-5-mini"
      ]
    }
  ],
  "Router": {
    "default": "qwen,qwen3-coder-plus",
    "background": "qwen,qwen3-coder-plus",
    "think": "qwen,qwen3-coder-plus",
    "longContext": "qwen,qwen3-coder-plus",
    "longContextThreshold": 800000,
    "webSearch": "gemini-cli,gemini-2.5-pro"
  }
}
